<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.localisation.vicomtech" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="WifiWizard">
        <param name="android-package" value="com.pylonproducts.wifiwizard.WifiWizard" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="WifiWizard2">
        <param name="android-package" value="wifiwizard2.WifiWizard2" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="AddressImpl">
        <param name="android-package" value="com.tv.plugin.AddressImpl" />
    </feature>
    <feature name="MacAddressPlugin">
        <param name="android-package" value="com.badrit.MacAddress.MacAddressPlugin" />
    </feature>
    <feature name="CheckGPS">
        <param name="android-package" value="de.fastr.phonegap.plugins.CheckGPS" />
    </feature>
    <feature name="RequestLocationAccuracy">
        <param name="android-package" value="cordova.plugin.RequestLocationAccuracy" />
    </feature>
    <feature name="Diagnostic">
        <param name="android-package" value="cordova.plugins.Diagnostic" />
        <param name="onload" value="true" />
    </feature>
    <name>IMHERE</name>
    <description>
        A sample Apache Cordova application that allow you to detect different area and labeled them.
    </description>
    <author email="fzola@vicomtech.org" href="http://effezeta88">
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Orientation" value="portrait" />
</widget>
